{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:shared": {
      "dependsOn": [
        "^build:shared"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:bot": {
      "dependsOn": [
        "^build:shared"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:api": {
      "dependsOn": [
        "^build:shared"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "type-check": {}
  }
}
